config gpio 'general'
	option debug '0'
	option isActive '0'

config gpio 'IO_0'
	option status 'enable'
	option direction 'in'	# Значения по умолчанию выставляются в правиле
	option value '0'		# для обеспечения безопасности "железа".
	option trigger 'none'
	option debounce_ms '100'
	option action_command 'echo "IO0 detected"'
	option hw_info 'usb-cp2112'
	

