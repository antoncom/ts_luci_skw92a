config gpio 'general'
	option debug '0'
	option isActive '0'

config gpio 'IO_0'
	option status 'disable'
	option direction 'out'
	option default_value_out '0'	
	option value '0'		
	option trigger 'none'
	option debounce_ms '100'
	option action_command 'echo "IO0 detected"'
	option hw_info 'usb-cp2112'
	

